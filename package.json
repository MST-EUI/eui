{
  "name": "@mistong/eui",
  "version": "0.1.0",
  "description": "React Components Implementation for EWT UI",
  "main": "dist/eui.js",
  "scripts": {
    "dist-dev": "cross-env NODE_ENV=development webpack --mode development --progress",
    "dist-pro": "webpack --mode production --progress",
    "build": "rm -rf dist && npm run dist-dev && npm run dist-pro",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MST-EUI/eui.git"
  },
  "keywords": [
    "react",
    "component"
  ],
  "author": "mst-eui team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MST-EUI/eui/issues"
  },
  "homepage": "https://github.com/MST-EUI/eui#readme",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "webpack": "^1.12.9",
    "webpack-cli": "^1.5.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@mistong/eui-button": "^0.1.3",
    "@mistong/eui-dialog": "^0.1.4"
  }
