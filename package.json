{
  "name": "@mistong/eui",
  "version": "0.1.15",
  "description": "An eui component",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run buildcss && eui server",
    "dev": "npm run buildcss && eui server",
    "build": "npm run buildcss && npm run buildlib && eui build",
    "buildcss": "node src/css.generate.js",
    "buildlib": "node src/lib.generate.js",
    "test": "eui test",
    "commit": "git-cz",
    "coverage": "eui test --coverage",
    "lint": "eslint --fix src/*",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "version": "npm run changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build"
  },
  "author": "mistong eui team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MST-EUI/eui.git"
  },
  "bugs": {
    "url": "https://github.com/MST-EUI/eui/issues"
  },
  "homepage": "https://github.com/MST-EUI/eui",
  "keywords": [
    "react",
    "react-component",
    "eui"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "files": [
    "dist",
    "src",
    "lib",
    "demo"
  ],
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@mistong/eui-cli": "latest",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.6.0",
    "chalk": "^2.4.1",
    "conventional-changelog-cli": "^2.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-14": "^1.1.0",
    "es5-shim": "^4.5.10",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "husky": "^0.14.3",
    "jest": "^23.2.0",
    "lint-staged": "^7.2.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.8.3",
    "react": "^0.14.9",
    "react-dom": "^0.14.9",
    "rimraf": "^2.6.2",
    "react-addons-test-utils": "^0.14.0"
  },
  "dependencies": {
    "@mistong/eui-code": "^0.1.4",
    "@mistong/eui-css": "latest",
    "@mistong/eui-demo": "^0.1.1",
    "@mistong/eui-empty-data": "^0.1.8",
    "@mistong/eui-icon": "^0.1.8",
    "classnames": "^2.2.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add ."
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "subject-case": [
        0
      ]
    }
  }
}
